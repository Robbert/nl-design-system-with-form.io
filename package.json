  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=. react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm-run-all --continue-on-error lint:**",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix --allow-empty-input '**/*.css'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --no-error-on-unmatched-pattern .",
    "lint:css": "stylelint --allow-empty-input '**/*.css'",
    "lint:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --no-error-on-unmatched-pattern .",
    "lint:prettier": "prettier --check .",
    "prettier": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@gemeente-denhaag/design-tokens-components": "0.2.3-alpha.12",
    "@utrecht/component-library-css": "1.0.0-alpha.71",
