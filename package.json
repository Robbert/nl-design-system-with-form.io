    "@utrecht/design-tokens": "1.0.0-alpha.64",
    "@utrecht/web-component-library-react": "1.0.0-alpha.70",
    "@utrecht/web-component-library-stencil": "1.0.0-alpha.70",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-mdx": "1.15.0",
    "eslint-plugin-react": "7.24.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "sass": "^1.38.0",
    "style-loader": "1.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2"
  }
